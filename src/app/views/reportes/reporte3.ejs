<!DOCTYPE html>
<html html lang="es">
  <head>
    <meta charset="utf-8" />
    <title>Reportes</title>
    <link 
      rel="icon" 
      type="image/png" 
      href="https://www.pucevirtual.edu.ec/wp-content/uploads/2017/01/iconoceleste.gif"
    />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
      integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('../home/header.ejs') %>

    <div class="row">
      <div class="col-md-6 mt-4 text-center">
        <table class="table table-striped table-bordered">
          <tr>
            <th>Artista</th>
            <th>Nombre de Artista</th>
            <th>Correo Electronico</th>
            <th>Fecha de Inicio</th>
            <th>Genero</th>
          </tr>
          <% for (var i = 0; i < reporte3.length; i ++) { %>
          <tr>
            <td><%- reporte3[i].Artista %></td>
            <td><%- reporte3[i].Nombre_Artista %></td>
            <td><%- reporte3[i].Correo_Electronico %></td>
            <td><%- reporte3[i].Fecha_Inicio %></td>
            <td><%- reporte3[i].Genero %></td>
          </tr>
          <% } %>
        </table>
      </div>
    </div>
  </body>
</html>
